{
  "require": {
    "php": ">=8.3",
    "slim/slim": "^4.13",
    "slim/psr7": "^1.6",
    "php-di/php-di": "^7.0",
    "vlucas/phpdotenv": "^5.6",
    "monolog/monolog": "^2.0",
    "predis/predis": "^1.1",
    "nesbot/carbon": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "endroid/qr-code": "^4.0",
    "respect/validation": "^2.2",
    "tuupola/cors-middleware": "^1.4",
    "middlewares/trailing-slash": "^2.0",
    "promphp/prometheus_client_php": "2.10"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "post-install-cmd": [
      "@php -r \"if(!is_dir('var/logs')) mkdir('var/logs', 0777, true);\""
    ],
    "openapi:build": "vendor/bin/openapi --format json --output public/openapi.json app/OpenApi app/Actions"
  },
  "require-dev": {
    "zircote/swagger-php": "^4.10",
    "league/openapi-psr7-validator": "^0.16",
    "doctrine/migrations": "3.8",
    "phpunit/phpunit": "11.1"
  }
}
